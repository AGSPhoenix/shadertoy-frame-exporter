{
    "manifest_version": 2,
    "name": "Shadertoy looping recorder",
    "version": "0.1",
    "description": "High quality timed video export from Shadertoy",
    "content_scripts": [
        {
            "matches": ["*://*.shadertoy.com/*"],
            "js": ["inject.js"],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        "main.js",
        "lib/FileSaver-1.3.3.min.js"
    ]
}
